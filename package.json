{
  "name": "smkit",
  "version": "0.9.1",
  "description": "中国国密算法（SM2, SM3, SM4, ZUC）的纯 TypeScript 实现 / Pure TypeScript implementation of Chinese national cryptographic algorithms (SM2, SM3, SM4, ZUC)",
  "type": "module",
  "main": "./dist/smkit.cjs",
  "module": "./dist/smkit.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/smkit.js",
      "require": "./dist/smkit.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "unpkg": "./dist/smkit.umd.js",
  "jsdelivr": "./dist/smkit.umd.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "demo": "npx serve .",
    "demo:vue": "cd demo-vue && npm run dev",
    "prepublishOnly": "npm run type-check && npm run test && npm run build"
  },
  "keywords": [
    "sm2",
    "sm3",
    "sm4",
    "zuc",
    "cryptography",
    "chinese-standard",
    "gm",
    "guomi",
    "eea3",
    "eia3",
    "lte"
  ],
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/linyuliu/smkit.git"
  },
  "bugs": {
    "url": "https://github.com/linyuliu/smkit/issues"
  },
  "homepage": "https://github.com/linyuliu/smkit#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^2.0.1"
  }
}
